<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/colorPrimaryDark"
    tools:ignore="ExtraText">

    <TextView
        android:id="@+id/choose_your"
        android:layout_width="256dp"
        android:layout_height="24dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="42dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="35dp"
        android:text="Choose your best photo"
        android:textAppearance="@style/choose_your" />

    <ImageView
        android:id="@+id/rectangle_10"
        android:layout_width="326dp"
        android:layout_height="278dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="42dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="77dp"
        android:background="@drawable/rectangle_10"
        app:srcCompat="@drawable/gb3"/>
        />

    <ImageView
        android:id="@+id/rectangle_11"
        android:layout_width="326dp"
        android:layout_height="278dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="42dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="393dp"
        android:background="@drawable/rectangle_11"
        app:srcCompat="@drawable/gb4"/>
        />

    <ImageView
        android:id="@+id/rectangle_17"
        android:layout_width="411dp"
        android:layout_height="211dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="496dp"
        android:background="@drawable/rectangle_13"
        />

    <ImageView
        android:id="@+id/rectangle_7"
        android:layout_width="198dp"
        android:layout_height="44dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="106dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="557dp"
        android:background="@drawable/rectangle_7"
        />

    <TextView
        android:id="@+id/next"
        android:layout_width="41dp"
        android:layout_height="22dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="184dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="568dp"
        android:text="@string/next"
        android:textAppearance="@style/next"
        android:gravity="top"
        />

    <View
        android:id="@+id/ellipse_2"
        android:layout_width="9dp"
        android:layout_height="9dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="183dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="646dp"
        android:background="@drawable/ellipse_2"
        />

    <View
        android:id="@+id/ellipse_1"
        android:layout_width="9dp"
        android:layout_height="9dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="201dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="646dp"
        android:background="@drawable/ellipse_1"
        />

    <View
        android:id="@+id/ellipse_3"
        android:layout_width="9dp"
        android:layout_height="9dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="219dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="646dp"
        android:background="@drawable/ellipse_3"
        />

</RelativeLayout>